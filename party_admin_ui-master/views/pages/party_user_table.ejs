          <div class="left_col scroll-view">
            <div class="navbar nav_title" style="border: 0;">
              <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Gentelella Alela!</span></a>
            </div>

            <div class="clearfix"></div>

            <!-- menu profile quick info -->
            <% include ../partials/menuprofile %>
            <!-- /menu profile quick info -->

            <br />

            <!-- sidebar menu -->
            <% include ../partials/fixed_sidebar %>
            <!-- /sidebar menu -->

            <!-- /menu footer buttons -->
            <% include ../partials/sidebar_footer %>
            
            <!-- /menu footer buttons -->
          </div>
        </div>

        <!-- top navigation -->
            <% include ../partials/top_navigation %>
                </div>
        <!-- /top navigation -->

        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3> Party Users</h3>
              </div>

              <div class="title_right">
                <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search for...">
                    <span class="input-group-btn">
                      <button class="btn btn-default" type="button">Go!</button>
                    </span>
                  </div>
                </div>
              </div>
            </div>

            <div class="clearfix"></div>
        <!--              update status modal-->
        
        <form class="modal fade" data-keyboard="false" data-backdrop="static" id="edit_status_modal">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
<!--                        <button type="button" class="close" data-dismiss="modal"><span>&times;</span></button>-->
                        <h4 class="modal-title"><span class="glyphicon glyphicon-pencil"></span></h4>
                    </div>
                        <div class="modal-body">
                        
                        <div class="form-group">
                              <h2 id="update_status" style="text-align=center">Updating Record</h2>
                        </div>
                    </div>
                    
                    <div class="modal-footer">
                    </div>
                </div>
            </div>
        </form>
              
        <!-- edit Modal -->
        <form class="modal fade" data-keyboard="false" data-backdrop="static" id="edit_record_modal">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><span>&times;</span></button>
                        <h4 class="modal-title"><span class="glyphicon glyphicon-pencil"></span> Edit Record</h4>
                    </div>
                        <div class="modal-body">
                        <div class="form-group">
                            <label >Id:  <label id="edit_id" ></label></label>
                            <!--                            <input type="text"  class="form-control" id="edit_id" disabled>-->
                        </div>
                        <div class="form-group">
                            <label>Name:  <label id="edit_name" ></label></label>
<!--                            <input type="text"  class="form-control" id="edit_name" disabled>-->
                        </div>
                        <div class="form-group">
                            <label>Name:  <label id="edit_email" ></label></label>
<!--                            <input type="text"  class="form-control" id="edit_name" disabled>-->
                        </div>
                        <div class="form-group">
                            <label>Phone No:  <label id="edit_phone_no" ></label></label>
<!--                            <input type="text"  class="form-control" id="edit_phone_no" disabled>-->
                        </div>
                         <div class="form-group">
                            <label>Created At:  <label id="edit_created_At" ></label></label>
<!--                            <input type="text"  class="form-control" id="edit_created_At">-->
                        </div>
                            
                        <div class="form-group">
                            <label>Status: 
                            <div class="custom-select"  style="display: inline">
					       <select id="edit_status" name="edit_status"> 
							<option value="1">Active</option> 
							<option value="0">Not Verified</option> 
							<option value="-1">Blocked</option> 
							
						</select>
					</div></label>
                        </div>
                        <div class="form-group">
                             <label class="custom-checkbox"><input class="custom_checkbox" type="checkbox" value="" id="edit_subscribed" disabled name="edit_subscribed"/> Subscribed
                        </label> 
                        </div>
                    </div>
                    
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-primary" id="submit_modal">Save changes</button>
                    </div>
                </div>
            </div>
        </form>

            <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>  <small></small></h2>
<!--                    <h2>Party Users  <small></small></h2>-->
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                          <li><a href="#">Settings 1</a>
                          </li>
                          <li><a href="#">Settings 2</a>
                          </li>
                        </ul>
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                
                      <table id="datatable-party_user" class="table table-striped table-bordered dt-responsive">
                        </table>
                    </div>
                </div>
              </div>

          </div>
            </div>
        <!-- /page content -->

        <!-- footer content -->
        <footer>
            <% include ../partials/footer %>

        </footer>
        <!-- /footer content -->
